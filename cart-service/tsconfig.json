{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "outDir": "./dist",
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "typeRoots": ["./node_modules/@types", "src/types"]
    },
    "include": ["./**/*.ts", "src/config/redis.js"],
    "exclude": ["node_modules"]
}
